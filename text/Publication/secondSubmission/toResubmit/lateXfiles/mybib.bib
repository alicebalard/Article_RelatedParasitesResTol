@article{albrechtova_sperm-related_2012,
	title = {Sperm-related phenotypes implicated in both maintenance and breakdown of a natural species barrier in the house mouse},
	volume = {279},
	issn = {0962-8452, 1471-2954},
	url = {http://rspb.royalsocietypublishing.org/content/279/1748/4803},
	doi = {10.1098/rspb.2012.1802},
	abstract = {The house mouse hybrid zone (HMHZ) is a species barrier thought to be maintained by a balance between dispersal and natural selection against hybrids. While the HMHZ is characterized by frequency discontinuities for some sex chromosome markers, there is an unexpected large-scale regional introgression of a Y chromosome across the barrier, in defiance of Haldane's rule. Recent work suggests that a major force maintaining the species barrier acts through sperm traits. Here, we test whether the Y chromosome penetration of the species barrier acts through sperm traits by assessing sperm characteristics of wild-caught males directly in a field laboratory set up in a Y introgression region of the HMHZ, later calculating the hybrid index of each male using 1401 diagnostic single nucleotide polymorphisms (SNPs). We found that both sperm count (SC) and sperm velocity were significantly reduced across the natural spectrum of hybrids. However, SC was more than rescued in the presence of the invading Y. Our results imply an asymmetric advantage for Y chromosome introgression consistent with the observed large-scale introgression. We suggest that selection on sperm-related traits probably explains a large component of patterns observed in the natural hybrid zone, including the Y chromosome penetration.},
	language = {en},
	number = {1748},
	urldate = {2014-04-08},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Albrechtová, Jana and Albrecht, Tomáš and Baird, Stuart J. E. and Macholán, Miloš and Rudolfsen, Geir and Munclinger, Pavel and Tucker, Priscilla K. and Piálek, Jaroslav},
	month = dec,
	year = {2012},
	pmid = {23055063},
	keywords = {Hybrid zone, male fitness, species barrier, sperm, Y introgression},
	pages = {4803--4810},
	file = {Full Text PDF:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/ANUILV2R/Albrechtová et al. - 2012 - Sperm-related phenotypes implicated in both mainte.pdf:application/pdf;PubMed Central Full Text PDF:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/67MVPKQ3/Albrechtová et al. - 2012 - Sperm-related phenotypes implicated in both mainte.pdf:application/pdf;Snapshot:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/5H3RNFAM/4803.html:text/html}
}

