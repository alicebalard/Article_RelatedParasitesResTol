	author = {Matsubayashi, Makoto and Tsuchida, Sayaka and Kobayashi, Atsushi and Shibahara, Tomoyuki and Nakamura, Hiroshi and Murata, Koichi and Ushida, Kazunari},
	month = dec,
	year = {2018},
	keywords = {Japanese rock ptarmigan},
	pages = {243--250},
	file = {ScienceDirect Full Text PDF:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/EJMF3VQ6/Matsubayashi et al. - 2018 - Molecular identification of two Eimeria species, E.pdf:application/pdf;ScienceDirect Snapshot:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/6GSQ44QN/S2213224418300415.html:text/html}
