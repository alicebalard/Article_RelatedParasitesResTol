@article{lilue_reciprocal_2013,
	title = {Reciprocal virulence and resistance polymorphism in the relationship between \textit{Toxoplasma~gondii} and the house mouse},
	volume = {2},
	issn = {2050-084X},
	url = {10.7554/eLife.01298},
	doi = {10.7554/eLife.01298},
	abstract = {Virulence in the ubiquitous intracellular protozoon Toxoplasma gondii for its natural intermediate host, the mouse, appears paradoxical from an evolutionary standpoint because death of the mouse before encystment interrupts the parasite life cycle. Virulent T. gondii strains secrete kinases and pseudokinases that inactivate the immunity-related GTPases (IRG proteins) responsible for mouse resistance to avirulent strains. Such considerations stimulated a search for IRG alleles unknown in laboratory mice that might confer resistance to virulent strains of T. gondii. We report that the mouse IRG system shows extraordinary polymorphic complexity in the wild. We describe an IRG haplotype from a wild-derived mouse strain that confers resistance against virulent parasites by interference with the virulent kinase complex. In such hosts virulent strains can encyst, hinting at an explanation for the evolution of virulence polymorphism in T. gondii.},
	urldate = {2020-03-25},
	journal = {eLife},
	author = {Lilue, Jingtao and Müller, Urs Benedikt and Steinfeldt, Tobias and Howard, Jonathan C},
	editor = {Weigel, Detlef},
	month = oct,
	year = {2013},
	keywords = {coevolution, IRG protein, Toxoplasma gondii, virulence},
	pages = {e01298},
	file = {Full Text PDF:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/DGSAPPD4/Lilue et al. - 2013 - Reciprocal virulence and resistance polymorphism i.pdf:application/pdf}
}

