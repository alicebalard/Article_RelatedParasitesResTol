	pmid = {25543667},
	pmcid = {PMC4301661},
	keywords = {Humans, Polymorphism, Single Nucleotide, Genome-Wide Association Study, Heterozygote, Mortality, Proportional Hazards Models},
	pages = {159},
	file = {Full Text:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/4AKQKANA/Bihlmeyer et al. - 2014 - Genetic diversity is a predictor of mortality in h.pdf:application/pdf}
