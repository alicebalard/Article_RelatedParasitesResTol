@article{simberloff_hybridization_1996,
	title = {Hybridization between native and introduced wildlife species: importance for conservation},
	volume = {2},
	issn = {0909-6396, 1903-220X},
	shorttitle = {Hybridization between native and introduced wildlife species},
	url = {https://bioone.org/journals/Wildlife-Biology/volume-2/issue-3/wlb.1996.012/Hybridization-between-native-and-introduced-wildlife-species--importance-for/10.2981/wlb.1996.012.full},
	doi = {10.2981/wlb.1996.012},
	abstract = {Molecular techniques show that hybridization can be a major conservation problem. Introgression can lessen genotypic variety and break up gene complexes coadapted to local environments. Gamete wastage can be a problem for hybridizing species even when gene flow does not occur. Birds and mammals introduced for game purposes have hybridized with native populations, a process often facilitated by habitat modification. Further, translocation and stock enhancement programs can lead to loss of well-adapted genotypes. These problems increase as populations become fragmented and isolated, because finding mates is harder and habitat differences that bar reproduction are obliterated. What to do about hybridization is sometimes not obvious. Even if the problem is seen as important, often little can be done. Culling may be impossible because of difficulties in recognizing introgressed individuals. Wildlife biologists and managers should work to prevent introductions and translocations if hybridization problems cannot be ruled out. Further, wildlife professionals should educate the public about evolutionary aspects of introductions and translocations, including hybridization and introgression.},
	number = {3},
	urldate = {2019-01-10},
	journal = {Wildlife Biology},
	author = {Simberloff, Daniel},
	month = sep,
	year = {1996},
	pages = {143--151},
	file = {Full Text PDF:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/EL4P67MS/Simberloff - 1996 - Hybridization between native and introduced wildli.pdf:application/pdf;Snapshot:/home/alice/.zotero/zotero/8rqjrnmd.default/zotero/storage/RRWMSD8J/wlb.1996.012.html:text/html}
}

